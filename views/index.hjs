<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
	<link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>
  </head>
  <body>
	<header>
		<h1>Welcome to <em>{{ title }}</em></h1>
	</header>
	<!--<h2>Projects:</h2>-->
	<section class='tdSelectGrid'>
			{{#projects}}
				<img id='{{ name }}' class='tdSelectSource' src='images/{{ mainImage }}' alt='{{ description }}' />
			{{/projects}}
	</section>
	<section class='tdSelectPreview'>
		{{#projects}}
			<article  id='{{ name }}-target' class='tdSelectTarget'>
				<header>
					<h1>{{title}}</h1>
				</header>
				<p>{{description}}</p>
			</article>
		{{/projects}}
	</section>
  </body>
</html>